{
  "name": "bitmovin-player-react-native",
  "version": "0.1.0",
  "description": "Official React Native bindings for Bitmovin's mobile Player SDKs.",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "source": "src/index.ts",
  "react-native": "src/index.ts",
  "author": "Bitmovin GmbH",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "RNBitmovinPlayer.podspec",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "test": "jest",
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{ts,tsx}\"",
    "format": "prettier --write .",
    "build": "tsup ./src/index.ts --dts --target es2020 --format cjs,esm -d lib",
    "example": "yarn --cwd example",
    "pods": "cd example && pod-install --quiet",
    "bootstrap": "yarn && yarn example && yarn pods"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": "https://github.com/bitmovin/player-react-native-bridge",
  "license": "MIT",
  "homepage": "https://github.com/bitmovin/player-react-native-bridge#readme",
  "devDependencies": {
    "@commitlint/config-conventional": "11.0.0",
    "@react-native-community/eslint-config": "3.0.2",
    "@types/jest": "26.0.24",
    "@types/lodash.omit": "4.5.0",
    "@types/react": "17.0.39",
    "@types/react-native": "0.67.7",
    "commitlint": "11.0.0",
    "eslint": "8.18.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "8.0.1",
    "jest": "26.6.3",
    "lint-staged": "13.0.3",
    "pod-install": "0.1.37",
    "prettier": "2.7.1",
    "react": "17.0.2",
    "tsup": "5.12.9",
    "typescript": "4.5.5"
  },
  "dependencies": {
    "lodash.omit": "4.5.0"
  },
  "peerDependencies": {
    "react": ">=17",
    "react-native": ">=0.64"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.(ts|tsx)": "eslint",
    "*.(ts|tsx|js|jsx|md|json|yml|yaml)": "prettier --write"
  }
}
